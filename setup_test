################ Zones setup ################

################ Subnets setup ################

################ Family Guy setup ################
host add familyguy 1.2.3.4 peter@griffin.no -hinfo 1 -comment "Family Guy is too awesome"

# A records
host a_add familyguy 11.22.33.44
host a_add familyguy 129.240.6.10
host a_add familyguy 129.240.6.16
host a_add familyguy 129.240.171.69

# AAAA records
host aaaa_add familyguy 1001:2002:3003:4004:aa:bb:cc:dd
host aaaa_add familyguy 1234::abcd
host aaaa_add familyguy 2002:2a34::cd

# CNAME records
host cname_add familyguy chris
host cname_add familyguy meg
host cname_add familyguy peter
host cname_add familyguy stewie
host cname_add familyguy louis
host cname_add familyguy brian

# LOC record
host loc_set familyguy.uio.no "59 56 23 N 10 43 50 E 80m"

# PTR record
host ptr_set 129.240.189.22 familyguy.uio.no
host ptr_set 129.240.6.22 familyguy
host ptr_set 7155::69 familyguy

# TTL record
host ttl_set familyguy.uio.no 33021
host ttl_set chris 300
host ttl_set meg 3049
host ttl_set peter 12345
host ttl_set stewie 666
host ttl_set louis 6969
host ttl_set brian 999

# TXT record
host txt_add familyguy.uio.no "seth macfarlane <3"
host txt_add chris "chris boogieeater"
host txt_add meg "shut up meg"
host txt_add peter "peter, king of petoria"
host txt_add stewie "victory shall be mine!"
host txt_add louis "loouis"
host txt_add brian "brian, the dog"


################ American Dad setup ################
host add stan 129.240.1.69 american@dad.no -hinfo 2
host add roger 129.240.2.69 american@dad.no -hinfo 3
host add steve 129.240.3.69 american@dad.no
host add francine 129.240.4.69 american@dad.no
host add haley 129.240.5.69 american@dad.no
host add claus 129.240.6.69 american@dad.no

# SRV record
host srv_add _tull._udp.familyguy 2 50 4932 stan
host srv_add _tull._udp.familyguy 1 34 4932 roger
host srv_add _tull._udp.familyguy 1 11 4932 steve
